{
  "name": "do-nogi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier --ignore-path .gitignore \"./**/*.+(ts|tsx)\" --write",
    "precommit:lint": "npm run lint:fix && npm run format",
    "clean": "rimraf .next",
    "nuke:install": "rimraf ./node_modules ./package-lock.json",
    "toc": "i18next-resources-for-ts toc -i ./public/locales/en -o ./@types/resources.ts",
    "merge": "i18next-resources-for-ts merge -i ./public/locales/en -o ./@types/resources.json",
    "interface": "i18next-resources-for-ts interface -i ./public/locales/en -o ./@types/resources.d.ts",
    "update-translation-types": "npm run toc && npm run merge && npm run interface",
    "upt": "npm run update-translation-types"
  },
  "dependencies": {
    "@types/node": "20.4.9",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "framer-motion": "^10.15.1",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-resources-for-ts": "^1.3.3",
    "next": "13.4.13",
    "next-i18next": "^14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.2",
    "typescript": "5.1.6",
    "universal-cookie": "^6.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3"
  }
}
